<?xml version="1.0" encoding="UTF-8"?>
<ehcache >
    <diskStore path="./java/cache" />
    <!-- DefaultCache setting. -->
    <defaultCache
            maxElementsInMemory="1000"
            eternal="true"
            clearOnFlush="true"
            timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            maxElementsOnDisk="1000000"
            overflowToDisk="true"
            memoryStoreEvictionPolicy="LRU">

    </defaultCache>
